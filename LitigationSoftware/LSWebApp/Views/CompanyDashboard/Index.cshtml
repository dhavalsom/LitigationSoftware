@model  LSWebApp.Models.CompanyDashboardModel
@{
	ViewBag.Title = "Company Dashboard";
	Layout = "~/Views/Shared/_LayoutDynemicMenu.cshtml";
}
<style>
	table, th, td {
		border: 1px solid black;
	}
</style>
<h3>Company Dashboard for @Model.CompanyObject.CompanyName</h3>

<table class="table table-hover">
	<thead>
		<tr>
			<th>Index</th>
			<th>Report</th>
			<th>Category</th>
		</tr>	
	</thead>
	<tbody>
		<tr class="table-active">
			<td>a)</td>
			<td> @Html.ActionLink("Litigation management - filing deadlines", "Charts",new { chartId = 1 }, null)</td>
			<td>- User category to be defined</td>
		</tr>
		<tr class="table-active">
			<td>b)</td>
			<td> @Html.ActionLink("Statement of tax related disclosure", "Charts", new { chartId = 2 }, null)</td>
			<td>- User category to be defined</td>
		</tr>
		<tr class="table-active">
			<td>c)</td>
			<td> @Html.ActionLink("ETR analysis", "Charts", new { chartId = 3 }, null)</td>
			<td>- User category to be defined</td>
		</tr>
		<tr class="table-active">
			<td>d)</td>
			<td> @Html.ActionLink("Discharge of tax liability", "Charts", new { chartId = 4 }, null)</td>
			<td>- User category to be defined</td>
		</tr>
		<tr class="table-active">
			<td>e)</td>
			<td> @Html.ActionLink("Return filing compliance", "Charts", new { chartId = 5 }, null)</td>
			<td>- User category to be defined</td>
		</tr>
		<tr class="table-active">
			<td>f)</td>
			<td> @Html.ActionLink("Key issues and trends", "Charts", new { chartId = 6 }, null)</td>
			<td>- User category to be defined</td>
		</tr>
		<tr class="table-active">
			<td>g)</td>
			<td> @Html.ActionLink("Working capital lock up", "Charts", new { chartId = 7 }, null)</td>
			<td>- User category to be defined</td>
		</tr>
		<tr class="table-active">
			<td>h)</td>
			<td>Group summary</td>
			<td>- User category to be defined</td>
		</tr>
		<tr class="table-active">
			<td>i)</td>
			<td>Company summary</td>
			<td>- User category to be defined</td>
		</tr>
	</tbody>
</table>


